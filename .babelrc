{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["Android >= 2.1", "iOS >= 4", "ie >= 8", "firefox >= 15"]
        }
      }],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ],

    "transform-vue-jsx",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": ["env", "es2015","stage-2"],
      "plugins": ["transform-vue-jsx", "istanbul"]
    }
  }
}
